pipeline{

  agent any //  Defines the jenkins agent to run the pipeline.


  // Stages will tracking the squenceing flow of the pipeline


  stages {

  stage ('verify branch')
  {steps {
  echo "$Git_Branch"
  }
}


  stage ('Build'){
  steps {
    echo 'Building...'
    //  please add build commands here  " mvn clean install "
    }
  }

    stage (' Deploy ') {


  steps  {
     echo 'Deploying...'
     'mvn clean install'
  }

  steps{
  'mvn clean install'
  }
  post {
  steps{
  success {
  'Application Lunched'
  }
  }
  steps{
  failure{
  'Application failure'
  }
  }

  }
 }



  }



}